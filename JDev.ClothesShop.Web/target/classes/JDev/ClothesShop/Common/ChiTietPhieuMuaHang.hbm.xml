<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="JDev.ClothesShop.DAO.ChiTietPhieuMuaHang" table="chitietphieumuahang">
        <composite-id>           
            <key-property name="MaPhieuMuaHang" column="MaPhieuMuaHang" type="integer"/>  
            <key-property name="MaSanPham" column="MaSanPham" type="integer"/>  
        </composite-id>
        
        <property name="Gia" type="double">
            <column name="Gia"/>
        </property>
        <property name="SoLuong" type="integer">
            <column name="SoLuong"/>
        </property>
        
    </class>
</hibernate-mapping>
